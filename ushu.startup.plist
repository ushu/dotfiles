<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>ushu.startup</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <!-- help Github Desktop to work with SSH -->
    <string>launchctl setenv GIT_SSH_COMMAND /usr/bin/ssh</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>

